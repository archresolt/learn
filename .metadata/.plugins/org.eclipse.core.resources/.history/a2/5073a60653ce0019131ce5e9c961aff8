package linkedlist;

import javax.xml.soap.Node;

public class SxtlinkedList01 {
	/*
	 * 自定义一个链表
	 * 
	 */
	private Node first;
	private Node last;
	
	private int size;

	
	
	public void add(Object obj){
		Node node = new Node(obj);
		if(first == null){
			//node.previous = null;
			//node.next = null;
			
			first = node;
			last = node;
		}
	}
}
