package linkedlist;

import java.util.LinkedList;

public class Test01 {
public static void main(String[] args){
//	LinkedList list = new LinkedList();
	LinkedList<String> list = new LinkedList<String>
	
	//添加元素
	list.add("中国");
	list.add("美国");
	list.add("日本");
	list.add("法国");
	for (String i : list){
		
	}
//	在链表的头尾添加元素
	list.addFirst("俄罗斯");
	list.addLast("德国");
	
	//返回链表开头的元素
	list.getFirst();
	list.getLast();
	
	//删除并返回链表开头的元素
	list.removeFirst();
	list.removeLast();
	
}
}