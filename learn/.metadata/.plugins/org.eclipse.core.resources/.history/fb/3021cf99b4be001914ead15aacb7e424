package worker;

public abstract class Item {
	private int size;
	private String name;

	public String getName() {
		return this.name;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}

	@Override
	public String toString() {
		// TODO 自动生成的方法存根
		return super.toString();
	}
}
