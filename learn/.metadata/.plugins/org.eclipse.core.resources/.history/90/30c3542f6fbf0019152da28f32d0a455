package worker;

public class Guizi {

	private int number;
	private String type;
	private String color;

	public Guizi() {
		super();
	}

	public void guizi(int number, String type, String color) {
		this.number = number;
		this.type = type;
		this.color = color;
	}

	public void setNumber(int nmuber) {
		this.number = number;

	}

	public int getNumber() {
		return number;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getType() {
		return type;
	}

	public void setColor(String color) {
		this.color = color;
	}

	public String getColor() {
		return color;

	}

	public String toString() {
		return "Number:" + number;
	}
}
